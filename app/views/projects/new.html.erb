<div class="row">
  <div class="col-lg-12">
    <h3>Create Project</h3>
  </div>
</div>

<div class="create">
  <div class="container ">
    
    <%= form_for @project do |f| %>  
      <div class="form-group">
        <%= f.label :name %><br> 
        <%= f.text_field :name, :class => ['form-control'] %>
      </div>  
      <%= f.submit "Create", :class=>"btn btn-default" %> 
    <% end %>
    
  </div>
</div>